<%- include("partials/header", {active: "Articles"}) %>

<div class="articles-list">
  <div class="heading-bubble">
    <h1>Articles</h1>
  </div>
  <div class="list-group">
    <% articles.forEach(article => { %>
      <a href="/articles/<%= article._id %>" class="list-group-item list-group-item-action"><%= article.title %></a>
    <% }) %>
    </div>
</div>

<%- include("partials/footer") %>
